            <script type="text/javascript">
							function renderLib(data) {
								var html = '';
								for(var i = 0; i < data.length && i < 3; i++){
							    var item = data[i];
							    
							    
							    html +=  '<dl class="relate_lib_list clearfix">';
				          html +=  '  <dt>';
				          html +=  '  	<a target="_blank" href="'+item.url+'">';
							    html +=  '    	<img src="'+item.logo+'">';
							    html +=  '  	</a>';
							    html +=  '  </dt>';
				          html +=  '  <dd>';
				          html +=  '  	<h5><a target="_blank" href="'+item.url+'">'+item.name.replace(/知识库/,'')+'</a></h5>';
				          html +=  '    <p><label><span>'+item.conCount+'</span><em>收录</em></label></p>';
				          html +=  '  </dd>';
				        	html +=  '</dl>';
							  }
							  
							  return html; 
							}
							
							$(function(){
								  document.domain = 'csdn.net';
								  ajaxProxy(proxy_url, {
							        headers: {
							            'X-acl-token': 'FdGFBxj3XMufW1DtRaB7cPrnT1MK'
							        },
							        type: "post",
							        url: "http://internalapi.csdn.net/knowledge/api/api/base/getInfoByKeyword",
							        contentType: "application/json",
							        dataType: "json",
							        success: function(data) {
							        	document.domain = 'csdn.net';
							            
												if (data.bases != undefined) {		
													$("#scrollDiv").append(renderLib(data.bases)).fadeIn();       
												} else {
													$('.recruit').remove();
												}
							        },
							        error: function(err){
							        	$('.recruit').fadeOut();
							        	console.log(err);
							        }
							    });
							})
							</script>
