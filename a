      $('#two1').hover(function(){
          $('#tech_day_rank').show();
      }, function(){
//          $('#tech_day_rank').hide();
      });

      $('#two2').hover(function(){
          $('#nontech_day_rank').show();
          $('#tech_day_rank').hide();
      }, function(){
//          $('#nontech_day_rank').hide();
      });

    </script>
